extends layout

block content
      .container-Found
        #Makeup-Artist1
            each n in resultMakeUpArtist
                a(href="profile?userid=" + n.email)
                    img.makeupArtist-picture(src=n.profilePicture  width='100' height='100' alt='')
                br
                b.makeupArtistInfo #{n.artistName}
                p Email Address: #{n.email}
                p city: #{n.city}
                p Address: #{n.address}
                p Tel: #{n.phoneNumber}
                p Makeup Specialty: #{n.makeUpSpeciality}
                br
